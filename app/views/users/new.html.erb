<br>
<h1>My Wino Friends</h1>

<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label "First Name" %><br>
    <%= f.text_field :first_name, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "Last Name" %><br>
    <%= f.text_field :last_name, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "Email" %><br>
    <%= f.text_field :email, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "City" %><br>
    <%= f.text_field :city, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "State" %><br>
    <%= f.text_field :state, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "Country" %><br>
    <%= f.text_field :country, class: "form-control" %><br>
  </div>
  <div class="form-group">
    <%= f.label "Picture URL" %><br>
    <%= f.text_field :picture_url, class: "form-control" %><br>
  </div>
  <%= f.submit "Sign Me Up!", class: "btn btn-default" %>
<% end %>

<%= link_to "I already have an account.", "#" %>

