<br>
<%= form_for @user do |f| %>
  <%= f.label "First Name" %>
  <%= f.text_label :first_name %><br>

  <%= f.label "Last Name" %>
  <%= f.text_label :last_name %><br>

  <%= f.label "Email" %>
  <%= f.text_label :email %><br>

  <%= f.label "City" %>
  <%= f.text_field :city %><br>

  <%= f.label "State" %>
  <%= f.text_field :state %><br>

  <%= f.label "Country" %>
  <%= f.text_field :country %><br>

  <%= f.label "Picture URL" %>
  <%= f.text_field :picture_url %><br>

  <%= f.label "Password" %>
  <%= f.password_field :password %><br>

  <%= f.label "Password Confirmation" %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Sign Me Up!" %>

<% end %>

