
<% @wines.each do |wine| %>
  <hr>
  <%= image_tag(wine.wine_url, size: "75x75") %><br>
  <%= "Name: #{wine.name}" %><br>
  <%= "Year: #{wine.year}" %><br>
  <%= "Description: #{wine.description}" %><br>
  <button><%= link_to "Edit", edit_user_wine_path(@user, wine) %></button>
  <button>Delete</button>
<% end %>

