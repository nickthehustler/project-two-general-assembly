
<% @wines.each do |wine| %>
  <hr>
  <%= image_tag(wine.wine_url, size: "75x75") %><br>
  <%= "Name: #{wine.name}" %><br>
  <%= "Year: #{wine.year}" %><br>
  <%= "Description: #{wine.description}" %><br>
  <%= button_to "Edit", edit_user_wine_path(@user, wine), class: "button", method: "get" %>
  <%= button_to "Delete", user_wine_path(@user, wine), class: "button", method: "delete" %>
<% end %>

